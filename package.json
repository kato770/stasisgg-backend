{
  "name": "stasisgg-backend",
  "version": "1.0.0",
  "description": "BFF for stasis.gg",
  "scripts": {
    "test": "jest -o --verbose",
    "test-all": "jest",
    "rm-layers": "rimraf layers",
    "zip-twisted": "node create_layers.js --moduleName twisted",
    "zip-axios": "node create_layers.js --moduleName axios",
    "zip-kayn": "node create_layers.js --moduleName kayn",
    "zip-all": "run-s rm-layers zip-twisted zip-axios zip-kayn",
    "sls": "cross-env NODE_ENV=production sls deploy --force",
    "dev": "cross-env NODE_ENV=development sls offline",
    "deploy": "cross-env NODE_ENV=production sls",
    "deploy-l": "cross-env NODE_ENV=production run-s zip-all sls"
  },
  "dependencies": {
    "kayn": "^0.10.2",
    "lodash": "^4.17.15",
    "source-map-support": "^0.5.10",
    "stasisgg-types": "^1.0.6",
    "twisted": "^1.18.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.45",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/webpack": "^4.41.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "axios": "^0.19.0",
    "bestzip": "^2.1.5",
    "cross-env": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.1.0",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.12.1",
    "serverless-webpack": "^5.2.0",
    "shelljs": "^0.8.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.8.3",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
